<link rel="stylesheet" href=".././public/css/home.css">
<article>
    <h1 id="title">Home</h1>
    <ul id="reviewList">
    {{#each reviews}}
        <li class="review">
            <section>
                <a href="/view/review/{{this._id}}">{{this.content}}</a>
                <a href="/view/dish/{{this.dishId}}">{{this.dishname}}</a>
                <a href="/view/user/{{this.userId}}">{{this.username}}</a>
            </section>
        </li>
    {{/each}}
    </ul>
</article>